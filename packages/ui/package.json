{
    "name": "@pushduck/ui",
    "version": "0.1.7",
    "type": "module",
    "description": "React UI components for pushduck - File upload components you can copy and paste into your apps",
    "author": "Abhay Ramesh",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/abhay-ramesh/pushduck.git",
        "directory": "packages/ui"
    },
    "homepage": "https://pushduck.dev",
    "keywords": [
        "react",
        "components",
        "ui",
        "file-upload",
        "s3",
        "nextjs",
        "tailwind",
        "typescript",
        "pushduck",
        "shadcn",
        "registry"
    ],
    "scripts": {
        "dev": "npx shadcn@canary build",
        "build": "pnpm build:registry",
        "build:registry": "npx shadcn@canary build",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "type-check": "tsc --noEmit"
    },
    "files": [
        "public/r/**/*",
        "registry.json",
        "README.md"
    ],
    "dependencies": {
        "clsx": "^2.1.1",
        "lucide-react": "^0.563.0",
        "pushduck": "workspace:*",
        "tailwind-merge": "^3.4.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.3.1",
        "@types/node": "^24.10.0",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "tailwindcss": "^4.1.14",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "tailwindcss": ">=3.0.0"
