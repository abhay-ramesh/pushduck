{
  "name": "@pushduck/cli",
  "version": "0.2.0",
  "type": "module",
  "description": "Official CLI for pushduck - Add S3 upload functionality to your Next.js project",
  "main": "dist/index.cjs",
  "bin": {
    "pushduck": "dist/index.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhay-ramesh/pushduck.git",
    "directory": "packages/cli"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "nextjs",
    "s3",
    "upload",
    "cli",
    "setup",
    "scaffold",
    "aws",
    "cloudflare",
    "digitalocean"
  ],
  "author": "Abhay Ramesh",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.913.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.1",
    "detect-package-manager": "^3.0.2",
    "dotenv": "^17.2.3",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.2",
    "handlebars": "^4.7.8",
    "inquirer": "^12.10.0",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "tsdown": "^0.15.8",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*",
    "templates/**/*"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
